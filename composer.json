{
    "name": "devtropolis/NovaCodeShipStatusCard",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Devtropolis\\NovaCodeShipStatusCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtropolis\\NovaCodeShipStatusCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
